@charset 'utf-8';

/**
 * [made by hangyangws]
 */

body {
    -moz-user-select: none;
}


/**
 * header
 */

.header-fixed .header {
    position: fixed;
    z-index: 4;
    -webkit-transform: translateZ(0);
}

.header {
    min-width: 1000px;
    border-bottom: 1px solid #efefef;
    background-color: rgba(255, 255, 255, .9);
}

.header-wrap {
    width: 95%;
}

.header-list {
    line-height: 60px;
}

.header-list.active,
.header-list:hover,
.header-down:hover .header-list,
.header-dest-con a:hover,
.header-down-con a:hover,
.header-dest:hover .header-list,
.header-btn {
    color: #bdb099;
}

.header-down:hover .i-arrow-b {
    background-position: -36px -36px;
}


/*目的地*/

.header-dest-down {
    top: 58px;
    left: -20px;
    overflow: hidden;
    height: 0;
    -webkit-transform: translateY(10px);
    -moz-transform: translateY(10px);
    -ms-transform: translateY(10px);
    -o-transform: translateY(10px);
    transform: translateY(10px);
}

.header-dest:hover .header-dest-down {
    padding: 5px 5px 0;
    height: auto;
    -webkit-transform: translateY(-4px) translateZ(0);
    -moz-transform: translateY(-4px);
    -ms-transform: translateY(-4px);
    -o-transform: translateY(-4px);
    transform: translateY(-4px) translateZ(0);
}

.header-dest:hover .header-dest-down,
.header-top-line::before {
    -webkit-transition: transform .4s, opacity .4s;
    -moz-transition: transform .4s, opacity .4s;
    -o-transition: transform .4s, opacity .4s;
    transition: transform .4s, opacity .4s;
}

.h-dest-arrow {
    display: inline-block;
    width: 18px;
    height: 18px;
    -webkit-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    transform: rotate(45deg);
    background-color: #fff;
    box-shadow: 0 0 1px #dfdfdf;
}

.h-dest-arrow::before {
    position: absolute;
    top: 6px;
    left: 0;
    display: block;
    width: 26px;
    height: 14px;
    content: '';
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    background-color: #fff;
}

.header-dest-con {
    line-height: 44px;
    top: -14px;
    width: 300px;
    background: #fff;
    background: -moz-linear-gradient(top, #fff 43px, #efefef 44px);
    background: -o-linear-gradient(top, #fff 43px, #efefef 44px);
    background: -webkit-linear-gradient(top, #fff 43px, #efefef 44px);
    background: -ms-linear-gradient(top, #fff 43px, #efefef 44px);
    background-size: 100% 44px;
    box-shadow: 0 0 3px #dfdfdf;
}

.header-dest-con li {
    width: 80px;
    padding-left: 20px;
}


/**
 * ************************************************************************
 */

.header-down-con {
    top: -800px;
    background-color: rgba(255, 255, 255, .9);
    box-shadow: 1px 1px 1px #efefef;
}

.header-down:hover .header-down-con {
    top: 60px;
}

.header-dest {
    width: 43px;
}

.header-btn {
    line-height: 32px;
    border: 1px solid #bdb099;
    background-color: transparent;
}

.header-btn:hover {
    color: #fff;
    background-color: #bdb099;
}

.header-top-line {
    position: relative;
}

.header-top-line::before {
    position: absolute;
    display: block;
    width: 100%;
    height: 3px;
    content: '';
    -webkit-transform: translateY(100%);
    -moz-transform: translateY(100%);
    -ms-transform: translateY(100%);
    -o-transform: translateY(100%);
    transform: translateY(100%);
    opacity: 0;
    background-color: #bdb099;
}

.header-top-line:hover::before {
    -webkit-transform: translateY(0) translateZ(0);
    -moz-transform: translateY(0);
    -ms-transform: translateY(0);
    -o-transform: translateY(0);
    transform: translateY(0) translateZ(0);
    opacity: 1;
}


/**
 * footer - old
 */

.footer {
    min-width: 1000px;
}

.footer-none .footer {
    display: none;
}

.footer-text {
    padding: 40px 50px 20px 50px;
}

.footer-text > ul,
.footer-contact > li {
    margin-right: 50px;
}

.footer-text > ul:last-child,
.footer-contact > li:last-child {
    margin-right: 0;
}

.footer-con {
    padding-bottom: 50px;
    border-top: 1px solid #a79d8a;
}


/*联系我们图标*/

.i-contact {
    width: 30px;
    height: 30px;
    background: url(../images/info/i-contact.png) no-repeat;
}

.i-facebook {
    background-position: 0 0;
}

.i-twitter {
    background-position: 0 -30px;
}

.i-service {
    background-position: 0 -60px;
}

.i-weibo {
    background-position: 0 -90px;
}

.i-weixin {
    background-position: 0 -120px;
}

.i-email {
    background-position: 0 -150px;
}

.wdf-wx {
    z-index: -1;
    top: -268px;
    left: -4px;
    -webkit-transition: opacity .4s, top .4s;
    -moz-transition: opacity .4s, top .4s;
    -o-transition: opacity .4s, top .4s;
    transition: opacity .4s, top .4s;
    opacity: 0;
    border: 1px solid #bdb099;
}

.footer-weixin:hover .wdf-wx {
    z-index: 1;
    top: -305px;
    opacity: 1;
}

.footer-tip {
    line-height: 20px;
    z-index: -1;
    top: -40px;
    left: -36px;
    min-width: 100px;
    height: 20px;
    color: #fff;
    background-color: #555;
}

.footer-tip span,
.wdf-wx span {
    position: absolute;
    bottom: -12px;
    left: 45px;
    display: block;
    width: 0;
    height: 0;
    border: 6px solid transparent;
    border-top: 6px solid #555;
}

.wdf-wx span {
    left: 8px;
    border-top: 6px solid #fff;
}

.footer-con li:hover .footer-tip {
    z-index: 2;
    top: -32px;
}


/**
 * footer
 */

.footer-i {
    width: 40px;
    height: 40px;
    background: url(../images/info/footer.png) no-repeat;
}

.i-weixin {
    background-position: 0 0;
}

.i-weibo {
    background-position: -40px 0;
}

.i-logo {
    width: 80px;
    height: 15px;
    background-position: -80px 0;
}


/**
 * others
 */

.h-down2 {
    position: relative;
    top: 2px;
}

.h-top2 {
    position: relative;
    top: -2px;
}

.i-head {
    margin-right: 2px;
}

.header-register::before {
    position: relative;
    top: 5px;
    display: inline-block;
    height: 20px;
    margin-right: 16px;
    content: '';
    border-left: 1px solid #bdb099;
}


/**
 * ************************************************************************
 */


/*注册模块*/

.register-wrap {
    width: 436px;
    height: 464px;
    background-color: #fafafa;
}

.register-con {
    padding: 40px 32px;
}


/*登录模块*/

.login-wrap {
    width: 436px;
    height: 387px;
    background-color: #fafafa;
}

.login-con,
.find-pass-con {
    padding: 40px 32px 48px 32px;
}


/*找回密码模块*/

.find-pass-wrap {
    width: 436px;
    height: 390px;
    background-color: #fafafa;
}

.register-wrap,
.login-wrap,
.find-pass-wrap {
    -webkit-transition: transform .6s, opacity .6s;
    -moz-transition: transform .6s, opacity .6s;
    -o-transition: transform .6s, opacity .6s;
    transition: transform .6s, opacity .6s;
    opacity: 1;
}

.register-wrap.active,
.login-wrap.active,
.find-pass-wrap.active {
    -webkit-transform: translateY(-60px) scale(.8);
    -moz-transform: translateY(-60px) scale(.8);
    -ms-transform: translateY(-60px) scale(.8);
    -o-transform: translateY(-60px) scale(.8);
    transform: translateY(-60px) scale(.8);
    opacity: .2;
}


/**
 * 登录注册共有模块
 */

.reg-log-err {
    color: #ff2c55;
}

.reg-log-submit {
    width: 370px;
    height: 46px;
    border: 1px solid #bdb099;
}

.reg-log-btn:hover {
    color: #bdb099;
    background-color: #fff;
}

.reg-log-err-wrap {
    border: 1px solid #ff2c55 !important;
}

.reg-log-border {
    overflow: hidden;
    border: 1px solid #ccc;
}


/*注册登录 - 标题*/

.reg-log-tit {
    line-height: 62px;
    padding: 0 32px;
}

.reg-log-tit h2 {
    font-size: 20px;
    font-weight: 100;
}

.reg-log-close {
    width: 22px;
    height: 22px;
}

.reg-log-close::before {
    position: absolute;
    top: 1px;
    left: 10px;
    display: inline-block;
    width: 2px;
    height: 20px;
    content: '';
    background-color: #444;
}

.reg-log-close::after {
    position: absolute;
    top: 10px;
    left: 1px;
    display: inline-block;
    width: 20px;
    height: 2px;
    content: '';
    background-color: #444;
}

.reg-log-close::before,
.reg-log-close::after {
    -webkit-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    transform: rotate(45deg);
}


/*登录注册 - 电话*/

.reg-log-tel {
    height: 48px;
    margin-bottom: 12px;
}

.tel-area {
    line-height: 50px;
    width: 60px;
    height: 48px;
    padding-left: 20px;
    border-right: 1px solid #ccc;
}

.reg-log-tel input {
    width: 160px;
    height: 48px;
}

.tel-err {
    line-height: 50px;
    max-width: 86px;
    height: 48px;
}


/*登录注册 - 验证码*/

.reg-captcha {
    margin-bottom: 12px;
}

.reg-captcha label {
    width: 258px;
}

.reg-captcha input {
    line-height: 48px;
    width: 76px;
}

.reg-captcha span {
    line-height: 48px;
    max-width: 110px;
}

.reg-captcha button {
    line-height: 46px;
    width: 103px;
    margin-top: 1px;
    border: 1px solid #bdb099;
}


/*登录注册 - 密码*/

.reg-log-pass input {
    height: 48px;
}

.reg-log-pass span {
    line-height: 48px;
    max-width: 100px;
}
